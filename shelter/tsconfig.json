{
  "compilerOptions": {
    "target": "ES5",
    "allowNonTsExtensions": true,
    "module": "commonjs",
    "sourceMap": true,
    "isolatedModules": false,
    "noEmitOnError": false,
    "rootDir": "client/",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "inlineSources": true
  },
  "compileOnSave": true,
  "filesGlob": [
   "**/*.ts"
  ],
  "atom": {
    "rewriteTsconfig": false
  }
}
